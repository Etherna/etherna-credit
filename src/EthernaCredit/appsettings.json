{
  "AllowedHosts": "*",

  "Application": {
    "CompactName": "ethernaCredit"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },

  "SsoServer": {
    "Clients": {
      "SsoServer": {
        "ClientId": "ethernaCreditSsoClientId"
      },
      "Webapp": {
        "ClientId": "ethernaCreditWebappClientId"
      }
    },
    "LoginPath": "/Identity/Account/Login",
    "RegisterPath": "/Identity/Account/Register"
  }
}
